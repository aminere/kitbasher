{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "no-trailing-whitespace": false,
        "arrow-parens": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "no-namespace": false,
        "no-module": false
    },
    "rulesDirectory": []
}